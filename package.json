{
  "name": "hellobooks",
  "version": "1.0.8",
  "description": "Simple Online Book Library",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "heroku-postbuild": "npm run build:webpack",
    "install:config": "npm prune && npm install",
    "lint": "markdownlint --config linters/.markdownlint.json",
    "release": "babel src --out-dir lib",
    "pretest": "npm run --silent lint && sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "migrate:test": "sequelize db:migrate --env test",
    "migrate": "sequelize db:migrate",
    "build": "babel server/ -d build",
    "build:webpack": "webpack --config ./webpack.config.prod.js --progress",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "NODE_ENV=test nyc mocha",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "test:front": "mocha --reporter progress tools/testSetup.js \"client/src/**/*.test.js\"",
    "start:dev": "nodemon ./server/index.js --watch server/ --exec babel-node --presets es2015,stage-2",
    "start": "babel-node ./server/index.js --presets es2015,stage-2",
    "server": "nodemon --exec babel-node -- ./bin/index.js",
    "start:front": "webpack-dev-server --open --config ./webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fidelisojeah/Hello-Books.git"
  },
  "keywords": [
    "style guide",
    "lint",
    "airbnb",
    "es6",
    "es2015",
    "react",
    "jsx"
  ],
  "author": "Fidelis Ojeah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fidelisojeah/Hello-Books/issues"
  },
  "homepage": "https://github.com/fidelisojeah/Hello-Books",
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "chai-things": "^0.2.0",
    "clean-webpack-plugin": "^0.1.16",
    "coveralls": "~2.7.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.2.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "prettier-eslint": "^6.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^16.1.1",
    "sequelize-cli": "^2.8.0",
    "sinon": "~1.8.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "cookie-encrypter": "^0.2.3",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint-config-airbnb": "^15.1.0",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.2.0",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
