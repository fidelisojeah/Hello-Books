    "react-native": "^0.48.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.1.0",
    "save": "^2.3.1",
    "sequelize": "^4.4.2",
