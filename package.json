    "shortid": "^2.2.8",
    "superagent": "^3.6.0",
    "sass-loader": "^6.0.6",
    "style-ext-html-webpack-plugin": "^3.4.5",
    "style-loader": "^0.18.2",
    "svg-inline-loader": "^0.8.0",
    "sweetalert": "^2.0.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.19.1"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
